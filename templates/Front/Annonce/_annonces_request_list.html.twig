                            </ul>
                        </div>
                    </div>
                    <div class="details">
                        <div class="tc_content">
                            <div class="dtls_headr">
                                {% if annonce.price is not null %}
                                    <a class="fp_price" href="{{ path('annonce_single_bdmk', {slug: annonce.slug}) }}">{{ annonce.formatPrice }}<small> F cfa</small></a>
                                {% endif %}
                            </div>
                            <p class="text-thm">{{ annonce.city.title }}</p>
                            <h4>{{ annonce.category.title }}</h4>
                            <p>{{ annonce.title }}</p>
                        </div>
                        <div class="fp_footer">
                            <div class="fp_pdate float-right">{{ duration(annonce.createdAt) }}</div>
                        </div>
                    </div>
                </div>
            {% endif %}
            {% if not annonce.pack %}
